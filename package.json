{
  "name": "jr-resume",
  "version": "1.0.0",
  "description": "Professional resume for Jonathan Reyes",
  "main": "index.js",
  "scripts": {
    "compile": "./node_modules/.bin/hackmyresume build resume.json TO dist/resume.md dist/resume.html dist/resume.doc",
    "watch": "./node_modules/.bin/fsmonitor -p '+resume.json' npm run compile",
    "publish": "./node_modules/.bin/resume publish --theme elegant"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jylinman/resume.git"
  },
  "author": "Jonathan Reyes",
  "bugs": {
    "url": "https://github.com/jylinman/resume/issues"
  },
  "homepage": "https://jonathanreyes.com",
  "dependencies": {
    "fsmonitor": "^0.2.4",
    "hackmyresume": "^1.2.2",
    "resume-cli": "^0.4.8"
  }
}
